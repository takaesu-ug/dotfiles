{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "on"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.defaultFormatter": "rebornix.ruby",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "breadcrumbs.enabled": true,
  "cSpell.userWords": [
    "awscdk",
    "awscli",
    "Dynalist",
    "Raycast",
    "tokubai",
    "Vimmer",
    "Wareki"
  ],
  "code-runner.runInTerminal": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.fontFamily": "'JetBrains Mono', 'BIZ UDGothic', 'Ricty Diminished Discord', Menlo, Monaco, 'Courier New'",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordWrap": "off",
  "emmet.includeLanguages": {
    "ejs": "html",
    "erb": "erb",
    "html": "html",
    "javascript": "javascriptreact",
    "vue": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 5000,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "flow.useNPMPackagedFlow": true,
  "git.ignoreMissingGitWarning": true,
  "git.suggestSmartCommit": false,
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true,
    "suppressLineUncommittedWarning": true,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": true,
  "go.formatTool": "goimports",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "hediet.vscode-drawio.offline": false,
  "hediet.vscode-drawio.theme": "atlas",
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.validate.enable": false,
  "json.schemas": [],
  "markdown-preview-enhanced.liveUpdate": false,
  "markdown.preview.breaks": true,
  "notebook.editorOptionsCustomizations": null,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    }
  ],
  "python.languageServer": "Default",
  "redhat.telemetry.enabled": true,
  "ruby.intellisense": "rubyLocate",
  "ruby.locate": {
    "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}",
    "include": "**/*.rb"
  },
  "ruby.useLanguageServer": true,
  "solargraph.useBundler": true,
  "tabnine.experimentalAutoImports": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.osx": "zsh",
  "textlint.autoFixOnSave": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "vim.disableExtension": false,
  "vim.easymotion": false,
  "vim.easymotionKeys": "sdfghjkloiurewnm",
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": [
        "g",
        "j"
      ],
      "before": [
        "j"
      ]
    },
    {
      "after": [
        "g",
        "k"
      ],
      "before": [
        "k"
      ]
    }
  ],
  "vim.statusBarColorControl": false,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.nativeTabs": true,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Hybrid Next (Gray Background)",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "material-icon-theme",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.schemas": {},
  "editor.inlineSuggest.enabled": true,
  "workbench.tree.indent": 16
}
