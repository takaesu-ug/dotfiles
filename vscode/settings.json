{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "application.shellEnvironmentResolutionTimeout": 30,
  "breadcrumbs.enabled": true,
  "color-highlight.markerType": "dot-after",
  "color-highlight.matchHslWithNoFunction": true,
  "color-highlight.matchRgbWithNoFunction": true,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    // "source.organizeImports": "explicit", これを有効にすると、importの順番が変わるので無効にする
    "source.addMissingImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.fontFamily": "'JetBrains Mono', 'BIZ UDGothic', 'Ricty Diminished Discord', Menlo, Monaco, 'Courier New'",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "on",
    "strings": "on"
  },
  "editor.renderControlCharacters": true,
  "editor.rulers": [80, 100, 120],
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordWrap": "off",
  "emmet.includeLanguages": {
    "ejs": "html",
    "erb": "erb",
    "html": "html",
    "javascript": "javascriptreact",
    "vue": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 5000,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.ignoreMissingGitWarning": true,
  "git.suggestSmartCommit": false,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.completionContext.typescript.mode": "on",
  "github.copilot.chat.localeOverride": "ja",
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.nextEditSuggestions.fixes": true,
  "githubPullRequests.pullBranch": "never",
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true,
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": true,
  "go.formatTool": "goimports",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "hediet.vscode-drawio.offline": false,
  "hediet.vscode-drawio.theme": "kennedy",
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.validate.enable": false,
  "json.schemas": [],
  "markdown-preview-enhanced.liveUpdate": false,
  "markdown.preview.breaks": true,
  "notebook.editorOptionsCustomizations": null,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    }
  ],
  "prettify-ts.maxDepth": 2,
  "python.languageServer": "Default",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 14,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.referencesCodeLens.enabled": true,
  "vim.disableExtension": false,
  "vim.easymotion": false,
  "vim.easymotionKeys": "sdfghjkloiurewnm",
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    }
  ],
  "vim.statusBarColorControl": false,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "window.nativeTabs": true,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "workbench.colorCustomizations": {
    "tab.activeBackground": "#626262"
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.tree.indent": 16,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.schemas": {},
  "roo-cline.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show"
  ],
  "rubyLsp.formatter": "rubocop_internal",
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "completion": true,
    "codeLens": true,
    "definition": true,
    "workspaceSymbol": true,
    "signatureHelp": true,
    "typeHierarchy": true
  },
  "prettier.bracketSameLine": true,
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.highlightActiveIndentation": "always",
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim"
}
