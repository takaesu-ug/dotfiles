{
  "editor.fontSize": 14,
  "editor.fontFamily": "'JetBrains Mono', 'BIZ UDGothic', 'Ricty Diminished Discord', Menlo, Monaco, 'Courier New'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.renderControlCharacters": true,
  "editor.rulers": [80, 100, 120],
  "editor.minimap.enabled": false,
  "emmet.includeLanguages": {
    "html": "html",
    "erb": "erb",
    "javascript": "javascriptreact",
    "vue": "html",
    "ejs": "html"
  },
  "terminal.integrated.fontSize": 14,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 5000,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "[markdown]": {
    "editor.wordWrap": "on"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "javascript.validate.enable": false,
  "flow.pathToFlow": "${workspaceRoot}/node_modules/.bin/flow",
  "flow.useNPMPackagedFlow": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "zsh",
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true,
    "suppressLineUncommittedWarning": true,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.currentLine.enabled": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.easymotion": false,
  "vim.easymotionKeys": "sdfghjkloiurewnm",
  "vim.statusBarColorControl": false,
  "vim.disableExtension": false,
  "window.nativeTabs": true,
  "editor.suggest.localityBonus": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Hybrid Next (Gray Background)",
  "breadcrumbs.enabled": true,
  "javascript.preferences.quoteStyle": "single",
  "emmet.triggerExpansionOnTab": true,
  "eslint.autoFixOnSave": true,
  "tslint.autoFixOnSave": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "ruby.intellisense": "rubyLocate",
  "ruby.locate": {
    "include": "**/*.rb",
    "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}"
  },
  "ruby.useLanguageServer": true,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "vim.useSystemClipboard": true,
  "solargraph.useBundler": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": -0.3,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "hediet.vscode-drawio.local-storage": {
    ".drawio-config": {
      "language": "",
      "customFonts": [],
      "libraries": "general;basic;arrows2;clipart;flowchart;er;uml;aws4;mscae;citrix;gcp2;kubernetes;bpmn",
      "customLibraries": [
        "L.scratchpad"
      ],
      "plugins": [],
      "recentColors": [
        "E6E6E6",
        "none"
      ],
      "formatWidth": "240",
      "createTarget": false,
      "pageFormat": {
        "x": 0,
        "y": 0,
        "width": 850,
        "height": 1100
      },
      "search": true,
      "showStartScreen": true,
      "gridColor": "#d0d0d0",
      "darkGridColor": "#6e6e6e",
      "autosave": true,
      "resizeImages": null,
      "openCounter": 0,
      "version": 18,
      "unit": 1,
      "isRulerOn": false,
      "ui": ""
    }
  }
}
